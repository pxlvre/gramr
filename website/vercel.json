{
  "redirects": [
    {
      "source": "/docs/(.*)",
      "destination": "https://pxlvre.github.io/nothung/$1"
    },
    {
      "source": "/docs",
      "destination": "https://pxlvre.github.io/nothung/"
    },
    {
      "source": "/sh",
      "destination": "/install.sh"
    }
  ],
  "headers": [
    {
      "source": "/install.sh",
      "headers": [
        {
          "key": "Content-Type",
          "value": "text/plain; charset=utf-8"
        },
        {
          "key": "Cache-Control",
          "value": "s-maxage=300, stale-while-revalidate"
        }
      ]
    },
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-XSS-Protection",
          "value": "1; mode=block"
        }
      ]
    }
  ]
}