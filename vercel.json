{
  "name": "gramr-installer",
  "alias": ["getgramr.pxlvre.dev"],
  "github": {
    "enabled": false
  },
  "routes": [
    {
      "src": "/docs/(.*)",
      "dest": "https://pxlvre.github.io/gramr/$1"
    },
    {
      "src": "/docs",
      "dest": "https://pxlvre.github.io/gramr/"
    },
    {
      "src": "/",
      "dest": "/install.sh",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8",
        "Cache-Control": "s-maxage=300, stale-while-revalidate"
      }
    },
    {
      "src": "/sh",
      "dest": "/install.sh",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8",
        "Cache-Control": "s-maxage=300, stale-while-revalidate"
      }
    },
    {
      "src": "/install.sh",
      "dest": "/install.sh",
      "headers": {
        "Content-Type": "text/plain; charset=utf-8",
        "Cache-Control": "s-maxage=300, stale-while-revalidate"
      }
    }
  ]
}